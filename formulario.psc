Proceso formulario
	Definir nombre,apellido,direccion Como Caracter;
	Definir rut Como Real;
	
	Escribir 'Bievenido, por favor indique su nombre';
	Leer Nombre;
	Escribir 'Por favor indique su apellido';
	Leer apellido;
	Escribir 'Por favor indique su RUT';
	Leer rut;
	Escribir 'Por favor indique su direccion';
	Leer direccion;
	
	Escribir "Buen dia, ", Nombre, ' ', apellido, ' su rut es ', rut, ' y vive en la direccion ', direccion;
	
FinProceso
